<!--登录注册-->
<div class="list main_min_height main_top_bottom">
	<div class="list_header list_width">
		<div class="list_header_title fontbold center">登录注册</div>
	</div>
	<div class="signal list_width">
		<img class="signal_img" src="/images/antenna2.png" alt="定位"/>
	</div>
	<div class="list_width">
		<div class="login_form" method="post" action="#">
			<div class="input_div">
				用户名<input class="input_bk input_name padding8px" type="text"/>
			</div>
			<div class="input_div">
				密码<input class="input_bk input_psw padding8px" type="password"/>
			</div>
			<div class="input_div">
				<input class="input_login cursor padding8px" type="button" value="登录"/>
			</div>
		</div>
	</div>

	
	<div class="login_form" method="post" action="#">
		<div class="input_div">
			用户名<input id="username" class="input_bk input_name padding8px" type="text"/>
		</div>
		<div class="input_div">
			密码<input id="password" class="input_bk input_psw padding8px" type="password"/>
		</div>
		<div class="input_div">
			确认<input id="password_re" class="input_bk input_psw padding8px" type="password"/>
		</div>
		<div class="input_div">
			邮箱<input id="email" class="input_bk input_psw padding8px" type="text"/>
		</div>
		<div class="input_div">
			<input id="btn_register" class="input_login cursor padding8px" type="button" value="注册"/>
		</div>
	</div> 
</div>